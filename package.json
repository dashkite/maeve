{
  "name": "@dashkite/maeve",
  "version": "0.0.0",
  "description": "Implement and test Lambdas more easily",
  "main": "build/node/src/index.js",
  "exports": {
    ".": {
      "browser": "./build/browser/src/index.js",
      "node": "./build/node/src/index.js"
    },
    "./lambda/*": {
      "browser": "./build/browser/src/lambda/*.js",
      "node": "./build/node/src/lambda/*.js"
    },
    "./node": {
      "browser": "./build/browser/src/node/index.js",
      "node": "./build/node/src/node/index.js"
    },
    "./emulator": {
      "browser": "./build/browser/src/lambda/emulator.js",
      "node": "./build/node/src/lambda/emulator.js"
    }
  },
  "imports": {
    "#sublime": "./build/node/src/sublime/index.js",
    "#helpers": "./build/node/src/helpers/index.js"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "genie test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dashkite/maeve.git"
  },
  "author": "Dan Yoder",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/dashkite/maeve/issues"
  },
  "homepage": "https://github.com/dashkite/maeve#readme",
  "dependencies": {
    "@dashkite/base64": "^1.0.1",
    "@dashkite/joy": "^0.3.11",
    "@dashkite/utf8": "^1.0.1",
    "http-status-codes": "^2.1.4"
  },
  "devDependencies": {
    "@dashkite/amen": "^0.1.2",
    "@dashkite/amen-console": "^0.0.1",
    "@dashkite/assert": "^0.1.3",
    "@dashkite/genie": "^0.5.4",
    "@dashkite/genie-presets": "^0.5.2"
  }
}
